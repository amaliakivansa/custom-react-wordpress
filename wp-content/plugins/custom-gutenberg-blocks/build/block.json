{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "customplugin/custom-gutenberg-blocks",
  "version": "1.0.0",
  "title": "Custom Gutenberg Block",
  "category": "common",
  "icon": "welcome-learn-more",
  "attributes": {
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./frontend.css",
  "render": "file:./render.php"
}